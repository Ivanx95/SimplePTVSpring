<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<fx:root styleClass="windowButton" stylesheets="@../resources/one/main/styles/miverva.fxml.css" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="mainBtn" mnemonicParsing="false" prefHeight="90.0" prefWidth="14.0" styleClass="main">
         <graphic>
            <VBox alignment="CENTER" prefHeight="82.0" prefWidth="113.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="57.0" fitWidth="84.0" pickOnBounds="true" preserveRatio="true" />
                  <Label fx:id="label" alignment="CENTER" prefHeight="40.0" prefWidth="178.0" text="Label" />
               </children>
            </VBox>
         </graphic>
      </Button>
   </children>
</fx:root>
